#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a, x, i, y;
    printf("Insira o numero inteiro que gostaria de avaliar\n");
    scanf ("%d", &a);
    if(a%2==0){
       printf ("\n %d é par", a);
        } else{
        printf ("\n %d é ímpar", a);
        }

    if (a==10){
      printf ("\n %d é exatamente 10", a);
    }
    if (a>10){
      printf ("\n %d é maior que 10", a);
    
    }

    if (a<10){
      printf ("\n %d é menor que 10",a);
    }

  x=a*2;
  printf ("\nO dobro de %d é %d",a, x);

  

  for (i=1; i<=a; i++){
    y=y+1;
  }
  if(y==2){
    printf ("\n %d é primo",a);
  } else{
    printf ("\n%d não é primo", a);
  }

    return 0;
}
